<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.svenfulenchek_wguscheduler">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SvenFulenchek_WGUScheduler">
        <activity
            android:name=".ui.UI.TermView"
            android:exported="false" />
        <activity
            android:name=".ui.UI.TermEditor"
            android:exported="false" />
        <activity
            android:name=".ui.UI.Assessments"
            android:exported="false"
            android:parentActivityName=".ui.UI.MainActivity" />
        <activity
            android:name=".ui.UI.Courses"
            android:exported="false"
            android:parentActivityName=".ui.UI.MainActivity" />
        <activity
            android:name=".ui.UI.TermsList"
            android:exported="false"
            android:parentActivityName=".ui.UI.MainActivity" />
        <activity
            android:name=".ui.UI.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>